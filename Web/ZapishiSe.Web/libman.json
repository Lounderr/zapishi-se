{
  "version": "1.0",
  "defaultProvider": "jsdelivr",
  "libraries": [
    {
      "library": "jquery@3.6.3",
      "destination": "wwwroot/lib/jquery/",
      "files": [
        "dist/jquery.min.js",
        "dist/jquery.js"
      ]
    },
    {
      "library": "bootstrap@5.3.2",
      "destination": "wwwroot/lib/bootstrap/",
      "files": [
        "dist/css/bootstrap.css",
        "dist/css/bootstrap.min.css", 
        "dist/js/bootstrap.bundle.js",
        "dist/js/bootstrap.bundle.min.js",
        "scss/**"
      ]
    },
    {
      "library": "jquery-validation@1.19.5",
      "destination": "wwwroot/lib/jquery-validation/",
      "files": [
        "dist/jquery.validate.js",
        "dist/jquery.validate.min.js"
      ]
    },
    {
      "library": "jquery-validation-unobtrusive@4.0.0",
      "destination": "wwwroot/lib/jquery-validation-unobtrusive/",
      "files": [
        "dist/jquery.validate.unobtrusive.js",
        "dist/jquery.validate.unobtrusive.min.js"
      ]
    },
    {
      "library": "slick-carousel@1.8.1",
      "destination": "wwwroot/lib/slick-carousel/",
      "files": [
        "slick/slick.js",
        "slick/slick.min.js",
        "slick/slick.css",
        "slick/slick.min.css",
        "slick/slick-theme.min.css",
        "slick/fonts/slick.eot",
        "slick/fonts/slick.svg",
        "slick/fonts/slick.ttf",
        "slick/fonts/slick.woff"
      ]
    },
    {
      "library": "zooming@2.1.1",
      "destination": "wwwroot/lib/zooming/",
      "files": [
        "build/zooming.min.js"
      ]
    },
    {
      "library": "jquery-sticky@1.0.4",
      "destination": "wwwroot/lib/jquery-sticky/",
      "files": [
        "jquery.sticky.min.js"
      ]
    },
    {
      "provider": "cdnjs",
      "library": "font-awesome@6.4.2",
      "destination": "wwwroot/lib/font-awesome/"
    }
  ]
}